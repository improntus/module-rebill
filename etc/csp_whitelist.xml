<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
        <policy id="default-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="rebill-to" type="host">*.rebill.to</value>
                <value id="rebill-dev" type="host">*.rebill.dev</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="rebill" type="host">*.rebill.to</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
